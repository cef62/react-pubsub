{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "jsx-quotes": 2,
    "id-length": 0,
    "radix": 0,
    "no-param-reassign": 0,
    "react/jsx-closing-bracket-location": 0
  },
  "plugins": [
    "react"
  ]
}
